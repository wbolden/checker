Premises
{
  A(x) & (B(x) & C(x))
}
Conclusions
{
  A(x) < E 1
  B(x) & C(x) < E 1
  B(x) < E 3
  C(x) < E 3
  A(x) & B(x) < I 2 4
  (A(x) & B(x)) & C(x)< I 6 5
}

